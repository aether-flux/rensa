{
  "name": "rensa",
  "version": "3.0.1",
  "description": "Rensa is a modern and smooth-to-use backend framework with file-based routing, Layers (middlewares), and a DX-first API for scalable apps.",
  "main": "dist/index.js",
  "bin": {
    "rensa": "./dist/cli/main.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/exports/types/index.js",
      "types": "./dist/exports/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/exports/utils/index.js",
      "default": "./dist/exports/utils/index.js",
      "types": "./dist/exports/utils/index.d.ts"
    },
    "./compose": {
      "import": "./dist/exports/compose/index.js",
      "default": "./dist/exports/compose/index.js",
      "types": "./dist/exports/compose/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "compose": ["dist/exports/compose/index.d.ts"]
    }
  },
  "files": [
    "dist",
    "rust/index.js",
    "rust/index.d.ts",
    "rust/index.node",
    "README.md",
    "LICENSE.txt",
    "CHANGELOG.md"
  ],
  "keywords": [
    "rensa",
    "backend",
    "framework",
    "node",
    "nodejs",
    "web framework",
    "server",
    "express alternative",
    "fast backend",
    "lightweight framework",
    "middleware",
    "http server",
    "router",
    "optimized routing",
    "performance",
    "scalable",
    "minimal",
    "efficient",
    "modern backend",
    "web server",
    "REST API",
    "api framework",
    "server-side",
    "javascript",
    "typescript",
    "node framework"
  ],
  "author": "Amartya Chowdhury (aether-flux)",
  "license": "MIT",
  "homepage": "https://rensa.vercel.app/",
  "bugs": {
    "url": "https://github.com/aether-flux/rensa/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aether-flux/rensa"
  },
  "type": "module",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "mime": "^4.0.6",
    "watchpack": "^2.4.2"
  },
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/aetherflux"
  },
  "peerDependencies": {
    "ejs": "^3.1.10"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.15.3",
    "jido": "^1.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
